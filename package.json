{
  "name": "bardle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "cy:open": "cypress open",
    "cy:run:chrome": "cypress run --browser chrome --spec cypress/tests/functional/*",
    "cy:run:firefox": "cypress run --browser firefox --spec cypress/tests/functional/*",
    "cy:run:edge": "cypress run --browser edge --spec cypress/tests/functional/*",
    "percy": "percy exec -- cypress run --spec cypress/tests/visual/*",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@cypress/react": "^5.12.5",
    "@percy/cli": "^1.2.1",
    "@percy/cypress": "^3.1.1",
