    "axios": "^0.27.2",
    "cypress": "^9.7.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-a11y-dialog": "^6.1.5",
    "react-countdown": "^2.3.2",
    "react-dom": "^18.0.0",
    "react-ga": "^3.3.0",
    "react-icons": "^4.3.1",
    "react-scripts": "5.0.1",
